                Title = "Farm Watermelon game",
                Default = false,
                Callback = function(a)
                    getgenv().omgjazfarmwatermelongame = a
                    task.spawn(function()
                        while true do
                            task.wait()
                            if not getgenv().omgjazfarmwatermelongame then
                                break
                            end
                            for i, v in pairs(game:GetService("Workspace")["Watermelon Game"].Melons:GetChildren()) do
