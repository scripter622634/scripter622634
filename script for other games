    Tabs.Untitledtaggame:AddButton({
        Title = "Ranxware Hub",
        Description = "",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Zer0ids/Qwerty/main/UntitledTag/NewScript.lua"))()
        end
    })

    Tabs.Vehiclelegends:AddButton({
        Title = "Autofarm",
        Description = "",
