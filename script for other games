    Tabs.Wartycoon:AddButton({
        Title = "Awaken Hub",
        Description = "",
        Callback = function()
            loadstring(game:HttpGet("https://raw.githubusercontent.com/Awakenchan/Misc-Release/main/WarTycoon"))()
